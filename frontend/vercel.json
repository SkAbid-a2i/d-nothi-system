{
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://d-nothi-backend.onrender.com"
  }
}